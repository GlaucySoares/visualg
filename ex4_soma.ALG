algoritmo "Soma"

var
   valorum: real
   valordois: real
   soma: real

inicio
   escreva ("Digite um valor inteiro: ")
   leia(valorum)
   escreva ("Digite um valor inteiro: ")
   leia(valordois)
   soma <- valorum + valordois


   Escreval( "A soma entre" , valorum, " e" , valordois , " é igual a" , soma. )

fimalgoritmo