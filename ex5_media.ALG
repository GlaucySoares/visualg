algoritmo "Média na disciplina"

var
   notaum: real
   notadois: real
   media: real

inicio
   escreva ("Digite sua primeira nota: ")
   leia(notaum)
   escreva ("Digite sua segunda nota: ")
   leia(notadois)
   media <- ( notaum + notadois ) / 2


   Escreval( "A media entre" , notaum, " e" , notadois , " é igual a" , media.)
   
fimalgoritmo