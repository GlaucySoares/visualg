algoritmo "Antecessor e Sucessor"

var
   numero: real
   sucessor: real
   antecessor: real
inicio
   escreva ("Digite um numero: ")
   leia (numero)
   antecessor <- numero - 1
   sucessor <- numero + 1
   
   Escreval( "O antecessor de " , numero, " é " , antecessor ,  " e o sucessor é " , sucessor.)
   
fimalgoritmo